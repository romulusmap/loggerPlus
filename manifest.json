{
    "content_scripts": [
        {
            "js": ["loggerPlus.js"],
            "css": ["css/main.css"],
            "matches": [
                "https://su-demo.ax4.com/ax4/servlet/debugConsole",
                "https://su-psdev1.ax4.com/ax4/servlet/debugConsole",
                "https://su.ax4.com/ax4/servlet/debugConsole",
                "https://su-airports-me.ax4.com/ax4/servlet/debugConsole",
                "http://logs.axit.pl:54321/current/*/ax4.log*"
