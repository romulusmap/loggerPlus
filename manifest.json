        }
    ],
    "icons": {
        "16": "favicon/favicon-16x16.png",
        "48": "favicon/favicon-32x32.png"
    },
    "manifest_version": 3,
    "name": "Logger Plus",
    "description": "Debug Console extension that helps navigate through the wall of text.",
    "version": "1.2.5",
    "web_accessible_resources": [
        {
            "resources": ["img/scroll.png", "img/arrow.png", "img/expand.png", "resources/jscolor-2.5.1/jscolor.js"],
            "matches": ["https://su-demo.ax4.com/*", "https://su-psdev1.ax4.com/*", "https://su.ax4.com/*", "https://su-airports-me.ax4.com/*", "http://logs.axit.pl:54321/*"]
