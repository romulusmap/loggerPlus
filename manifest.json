{
    "content_scripts": [
        {
            "js": ["loggerPlus.js"],
            "css": ["css/main.css"],
            "matches": ["https://su-demo.ax4.com/ax4/servlet/debugConsole", "https://su-psdev1.ax4.com/ax4/servlet/debugConsole"]
        }
    ],
    "icons": {
        "16": "favicon/favicon-16x16.png",
        "48": "favicon/favicon-32x32.png"
    },
    "manifest_version": 3,
    "name": "Logger Plus",
    "description": "Debug Console extension that helps navigate through the wall of text.",
    "version": "1.1.0",
    "web_accessible_resources": [
        {
          "resources": ["img/scroll.png", "img/arrow-right.png"],
          "matches": ["https://su-demo.ax4.com/*", "https://su-psdev1.ax4.com/*"]
        }
    ]
}
